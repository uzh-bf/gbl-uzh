{
  "name": "@gbl-uzh/demo-game",
  "version": "0.2.1",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "npm run prisma:aurora prisma:generate",
    "build": "run-s build:nexus build:graphql build:next",
    "build:nexus": "ts-node --esm --transpile-only -P tsconfig.nexus.json src/pages/api/graphql.ts",
    "build:graphql": "graphql-codegen --config codegen.ts",
    "build:next": "cross-env NODE_ENV=production next build",
    "predev": "npm run prisma:aurora prisma:generate build:nexus",
    "dev": "run-p dev:*",
    "dev:graphql": "graphql-codegen --config codegen.ts --watch",
    "dev:nexus": "ts-node-dev --transpile-only --no-notify src/pages/api/graphql.ts",
    "dev:next": "next dev",
    "prisma:aurora": "aurora",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node --esm prisma/seed.ts",
    "start": "next start",
    "check": "npm-run-all --parallel check:ts format:ci lint",
    "check:ts": "tsc",
    "check:fix": "npm-run-all check:ts format lint:fix",
    "format": "prettier --write \"src/**/*.(js|ts|tsx|graphql|json)\" \"*.js\" \"*.ts\"",
    "format:ci": "prettier --list-different \"src/**/*.(js|ts|tsx|graphql|json)\" \"*.js\" \"*.ts\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --watch",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-regular-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@gbl-uzh/platform": "0.2.10",
    "@next-auth/prisma-adapter": "1.0.5",
    "@next/font": "13.0.3",
    "@prisma/client": "4.6.1",
    "@uzh-bf/design-system": "0.0.79",
    "date-fns": "2.29.3",
    "formik": "2.2.9",
    "graphql": "16.6.0",
    "graphql-yoga": "3.0.0-next.12",
    "jsonwebtoken": "8.5.1",
    "next": "13.0.3",
    "next-auth": "4.16.4",
    "next-i18next": "12.1.0",
    "nexus": "1.3.0",
    "nookies": "2.5.2",
    "prisma": "4.6.1",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "1.8.0",
    "ts-pattern": "4.0.5",
    "yup": "1.0.0-beta.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/fragment-matcher": "3.3.1",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typed-document-node": "2.3.6",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-operations": "2.5.6",
    "@graphql-codegen/typescript-resolvers": "2.7.6",
    "@tsconfig/node18": "1.0.1",
    "@tsconfig/recommended": "1.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "jest": "29.3.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.2.0",
    "prisma-aurora": "1.3.9",
    "tailwindcss": "3.2.4",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
