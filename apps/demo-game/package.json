{
  "name": "@gbl-uzh/demo-game",
  "version": "0.4.3",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "run-s prisma:copy prisma:generate",
    "build": "run-s build:nexus build:graphql build:next",
    "build:nexus": "ts-node --esm --transpile-only -P tsconfig.json src/pages/api/graphql.ts",
    "build:graphql": "graphql-codegen --config codegen.ts",
    "build:next": "cross-env NODE_ENV=production next build",
    "predev": "run-s prisma:copy prisma:generate build:nexus",
    "dev": "run-p dev:*",
    "dev:graphql": "graphql-codegen --config codegen.ts --watch",
    "dev:nexus": "ts-node-dev --transpile-only --no-notify src/pages/api/graphql.ts",
    "dev:next": "next dev",
    "prisma:copy": "ts-node prisma/copy.ts",
    "prisma:deploy": "doppler run --config prd -- prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:studio:prod": "doppler run --config prd -- prisma studio",
    "prisma:seed": "ts-node --esm prisma/seed.ts",
    "prisma:seed:prod": "doppler run --config prd -- ts-node --esm prisma/seed.ts",
    "start": "next start",
    "check": "npm-run-all --parallel check:ts format:ci lint",
    "check:ts": "tsc",
    "check:fix": "npm-run-all check:ts format lint:fix",
    "format": "prettier --write \"src/**/*.(js|ts|tsx|graphql|json)\" \"*.js\" \"*.ts\"",
    "format:ci": "prettier --list-different \"src/**/*.(js|ts|tsx|graphql|json)\" \"*.js\" \"*.ts\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --watch",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apollo/client": "3.7.15",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@gbl-uzh/platform": "*",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "4.16.1",
    "@uzh-bf/design-system": "1.8.0",
    "date-fns": "2.30.0",
    "formik": "2.4.0",
    "graphql": "16.7.1",
    "graphql-sse": "2.2.0",
    "graphql-yoga": "4.0.1",
    "i18next": "22.5.0",
    "jsonwebtoken": "9.0.0",
    "next": "13.4.6",
    "next-auth": "4.22.1",
    "next-i18next": "13.2.2",
    "nexus": "1.3.0",
    "nookies": "2.5.2",
    "prisma": "4.16.1",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-dice-complete": "2.1.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.3.1",
    "tailwind-merge": "1.12.0",
    "ts-pattern": "4.3.0",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.0",
    "@graphql-codegen/fragment-matcher": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typed-document-node": "5.0.0",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-operations": "4.0.0",
    "@graphql-codegen/typescript-resolvers": "4.0.0",
    "@tsconfig/node18": "1.0.1",
    "@tsconfig/recommended": "1.0.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "10.4.14",
    "cross-env": "7.0.3",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "jest": "29.5.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "tailwindcss": "3.3.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.3"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
